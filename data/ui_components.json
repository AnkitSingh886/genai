[
    {
        "image": "dashboard.png",
        "components": {
            "UI_Components": [
                {
                    "component_name": "Button",
                    "position": "top-right",
                    "color": "#007bff",
                    "size": "medium",
                    "text": "Submit",
                    "action": "Sends form data to API"
                },
                {
                    "component_name": "Modal",
                    "position": "center",
                    "background_color": "#ffffff",
                    "border_radius": "8px",
                    "header_text": "Confirm Action",
                    "body_text": "Are you sure you want to proceed?",
                    "buttons": [
                        "Yes",
                        "Cancel"
                    ]
                },
                {
                    "component_name": "Card",
                    "position": "left",
                    "width": "300px",
                    "height": "200px",
                    "background_color": "#f8f9fa",
                    "border_radius": "4px",
                    "padding": "12px"
                },
                {
                    "component_name": "Form",
                    "position": "center",
                    "fields": [
                        {
                            "name": "Username",
                            "type": "input",
                            "placeholder": "Enter username"
                        },
                        {
                            "name": "Password",
                            "type": "input",
                            "placeholder": "Enter password",
                            "password": true
                        }
                    ],
                    "submit_button": {
                        "text": "Login",
                        "color": "#007bff"
                    }
                }
            ],
            "Layout": {
                "grid_system": "12-column",
                "spacing": "16px",
                "margins": "8px",
                "paddings": "12px"
            },
            "Typography": {
                "font_family": "Inter",
                "heading_size": "24px",
                "body_size": "16px",
                "bold": true
            },
            "Colors": {
                "primary": "#007bff",
                "secondary": "#6c757d",
                "background": "#f8f9fa",
                "success": "#28a745",
                "error": "#dc3545"
            },
            "Interactions": {
                "hover_effect": "fade-in",
                "click_action": "submit form data",
                "animation": "fadeIn"
            }
        }
    },
    {
